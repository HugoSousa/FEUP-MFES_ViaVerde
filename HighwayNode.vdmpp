class HighwayNode is subclass of Node
types
	public Type = <ENTRANCE> | <EXIT>;
	
instance variables
	public type: Type;
	
operations
	public HighwayNode : nat1 * nat1 * Type ==> HighwayNode
	 HighwayNode(lat, long, t) == (
	 	local := mk_Local(lat, long);
	 	type := t;
		return self;
	);
	
end HighwayNode